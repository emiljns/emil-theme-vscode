		"editorSuggestWidget.selectedForeground": "#f5f5f5",
		"editorSuggestWidget.highlightForeground": "#fc595f",
		"editorHoverWidget.background": "#202020",
		"editorHoverWidget.border": "#252525",
		"editorHoverWidget.foreground": "#a1b0b8",
		"editorHoverWidget.statusBarBackground": "#252525"
	},
	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#5d6f71"
			}
		},
		{
			"scope": ["string", "constant.other.symbol"],
			"settings": {
				"foreground": "#fc595f"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
			"settings": {
				"foreground": "#d3494e"
			}
		},
		{
			"scope": ["variable", "variable.other"],
			"settings": {
				"foreground": "#a1b0b8"
			}
		},
		{
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#a63c40"
			}
		},
		{
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#df9395"
			}
		},
		{
			"scope": ["entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#ba8586"
			}
		}
	]
}
